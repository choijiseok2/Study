<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inittest.mapper.ChatMapper">

	
	<insert id="insertChatHis" parameterType="com.inittest.vo.ChatMessage">
		INSERT INTO T_CHAT_HIS (  
			sender,
			content,
			type
		)VALUES(
			#{sender},
			#{content},
			#{type}
		)
	</insert>
	
	
	
</mapper>